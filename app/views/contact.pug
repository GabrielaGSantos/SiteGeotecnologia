html
	head
		title Contact - Alpha by HTML5 UP
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		//if lte IE 8
			script(src="assets/js/ie/html5shiv.js")
		link(rel="stylesheet", href="assets/css/main.css")
		//if lte IE 8
			link(rel="stylesheet", href="assets/css/ie8.css")
	body
		#page-wrapper
			// Header
			header#header
				h1
					a(href="index.html") Alpha
					|  by HTML5 UP
				nav#nav
					ul
						li
							a(href="index.html") Home
						li
							a.icon.fa-angle-down(href="#") Layouts
							ul
								li
									a(href="generic.html") Generic
								li
									a(href="contact.html") Contact
								li
									a(href="elements.html") Elements
								li
									a(href="#") Submenu
									ul
										li
											a(href="#") Option One
										li
											a(href="#") Option Two
										li
											a(href="#") Option Three
										li
											a(href="#") Option Four
						li
							a.button(href="#") Sign Up
			// Main
			section#main.container(class="75%")
				header
					h2 Contact Us
					p Tell us what you think about our little operation.
				.box
					form(method="post", action="#")
						.row.uniform(class="50%")
							.6u(class="12u(mobilep)")
								input#name(type="text", name="name", value="", placeholder="Name")
							.6u(class="12u(mobilep)")
								input#email(type="email", name="email", value="", placeholder="Email")
						.row.uniform(class="50%")
							.12u
								input#subject(type="text", name="subject", value="", placeholder="Subject")
						.row.uniform(class="50%")
							.12u
								textarea#message(name="message", placeholder="Enter your message", rows="6")
						.row.uniform
							.12u
								ul.actions.align-center
									li
										input(type="submit", value="Send Message")
			// Footer
			footer#footer
				ul.icons
					li
						a.icon.fa-twitter(href="#")
							span.label Twitter
					li
						a.icon.fa-facebook(href="#")
							span.label Facebook
					li
						a.icon.fa-instagram(href="#")
							span.label Instagram
					li
						a.icon.fa-github(href="#")
							span.label Github
					li
						a.icon.fa-dribbble(href="#")
							span.label Dribbble
					li
						a.icon.fa-google-plus(href="#")
							span.label Google+
